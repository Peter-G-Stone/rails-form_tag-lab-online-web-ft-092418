 <h3>Student Form</h3>

<% if @student.first_name %>
    <h4><%= @student.to_s %></h4>
<% end %>

<%= form_tag "/students/new" do %>
    <label>First name:</label><br>
    <%= text_field_tag :'first_name' %> <br>

    <label>Last name:</label><br>
    <%= text_field_tag :'last_name' %> <br>


    <%= submit_tag "Submit Student" %>
<% end %>